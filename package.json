{
  "name": "backbone-starter",
  "version": "1.0.0",
  "description": "A starter project for Backbone.js with ES6, Webpack, Handlebars, Bootstrap and Mocha libraries.",
  "scripts": {
    "build": "node node_modules/webpack/bin/webpack -d",
    "build-dev": "node node_modules/webpack-dev-server/bin/webpack-dev-server --content-base bin/",
    "build-prod": "node node_modules/webpack/bin/webpack -p",
    "test": "mocha --recursive -r babel-register"
  },
  "keywords": [
    "backbone.js",
    "ES6",
    "webpack",
    "handlebars",
    "lodash",
    "mocha",
    "chai",
    "eslint"
  ],
  "author": "Jérémy Buget",
  "license": "MIT",
  "dependencies": {
    "backbone": "^1.3.3",
    "handlebars": "^4.0.5",
    "jquery": "^3.1.0",
    "lodash": "^4.15.0"
  },
  "devDependencies": {
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.14.0",
    "babel-register": "^6.14.0",
    "chai": "^3.5.0",
    "css-loader": "^0.25.0",
    "eslint": "^3.5.0",
    "eslint-config-airbnb-base": "^7.0.0",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-import": "^1.14.0",
    "handlebars-loader": "^1.4.0",
    "html-webpack-plugin": "^2.22.0",
    "mocha": "^3.0.2",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.15.1"
  }
}
